<% @user.authored_articles.each do |article| %>
  <div class="show-user-article-list-options">
    <div class="show-user-article-list">
      <%= link_to article.title, article_path(article) %>
    </div>
    <div class="show-user-article-options">
      <%= show_user_options(article) %>
    </div>
  </div>
<% end %>